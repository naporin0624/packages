{
  "name": "@naporin0624/react-flowder",
  "version": "3.2.13",
  "description": "data loader for react",
  "keywords": [
    "react",
    "suspense",
    "rxjs",
    "state",
    "state management"
  ],
  "author": "naporin0624 <naporin0624@gmail.com>",
  "homepage": "https://github.com/naporin0624/packages/tree/main/packages/react-flowder#react-flowder",
  "license": "MIT",
  "main": "lib/index-cjs.js",
  "module": "lib/index-esm.js",
  "types": "lib/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index-esm.js",
      "require": "./lib/index-cjs.js"
    },
    "./utils": {
      "types": "./lib/utils/index.d.ts",
      "import": "./lib/utils/index-esm.js",
      "require": "./lib/utils/index-cjs.js"
    },
    "./apollo": {
      "types": "./lib/apollo/index.d.ts",
      "import": "./lib/apollo/index-esm.js",
      "require": "./lib/apollo/index-cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      "apollo": [
        "lib/apollo/index.d.ts"
      ],
      "utils": [
        "lib/utils/index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "lib",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "lint:fix": "yarn lint --fix",
    "prebuild": "tsc --noEmit && rimraf lib",
    "build": "rollup -c ./rollup.config.js"
  },
  "private": false,
  "peerDependencies": {
    "@apollo/client": "*",
    "graphql": "*",
    "react": ">=16.8",
    "rxjs": "*"
  },
  "peerDependenciesMeta": {
    "@apollo/client": {
      "optional": true
    },
    "graphql": {
      "optional": true
    }
  },
  "devDependencies": {
    "@apollo/client": "^3.5.10",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/react": "^17.0.37",
    "canvas": "^2.9.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "graphql": "^16.3.0",
    "jsdom": "^19.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.5.5"
  },
  "dependencies": {
    "@naporin0624/simple-store": "^0.1.2",
    "fast-json-stable-stringify": "^2.1.0",
    "use-sync-external-store": "^1.0.0"
  }
}
