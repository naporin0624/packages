{
  "name": "@naporin0624/cache-manager",
  "version": "0.0.2",
  "description": "A simple function memoization management library.",
  "keywords": [
    "memo",
    "react"
  ],
  "author": "naporin0624 <naporin0624@gmail.com>",
  "homepage": "https://github.com/naporin0624/packages/tree/main/packages/cache-manager#cache-manager",
  "main": "lib/index-cjs.js",
  "module": "lib/index-esm.js",
  "types": "lib/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index-esm.js",
      "require": "./lib/index-cjs.js"
    },
    "./react": {
      "types": "./lib/react/index.d.ts",
      "import": "./lib/react/index-esm.js",
      "require": "./lib/react/index-cjs.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "scripts": {
    "test": "jest --passWithNoTests",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "lint:fix": "yarn lint --fix",
    "prebuild": "tsc --noEmit && rimraf lib",
    "build": "rollup -c ./rollup.config.js"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "@types/react": "^17.0.37",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@naporin0624/simple-store": "^0.0.5",
    "fast-json-stable-stringify": "^2.1.0"
  },
  "license": "MIT"
}
